{
	"private": true,
	"name": "stylegpt",
	"description": "ðŸ¤–ðŸ§© styleGPT is an extension that change style from chatGPT web. ðŸ¦ðŸŒˆ",
	"version": "1.3.0",
	"type": "module",
	"engines": {
		"node": "^12.17.0 || ^14.13 || >=16.0.0",
		"pnpm": ">=3"
	},
	"packageManager": "pnpm@7.26.0",
	"src": [
		"package.json"
	],
	"scripts": {
		"scripts": "pnpm run",
		"lint": "node .dev/lint.js --check",
		"lint:fix": "node .dev/lint.js --fix",
		"build": "node .dev/build.js --type all",
		"build:firefox": "node .dev/build.js --type firefox",
		"build:chrome": "node .dev/build.js --type chrome",
		"build:chrome-mv2": "node .dev/build.js --type chrome-mv2",
		"dev": "node .dev/build.js --watch",
		"dev:firefox": "node .dev/build.js --watch --type firefox",
		"dev:chrome": "node .dev/build.js --watch --type chrome",
		"dev:chrome-mv2": "node .dev/build.js --watch --type chrome-mv2",
		"ask:lint": "node .dev/lint.js",
		"ask:dev": "pnpm dev",
		"ask:build": "node .dev/build.js",
		"coverage": "browserslist --coverage",
		"push-main": "node .dev/release.js --no-release",
		"release": "node .dev/release.js",
		"readme": "node .dev/readme.js",
		"todo": "node .dev/todo.js",
		"dev-info": "md docs/dev-info.md",
		"test": "playwright test",
		"cm": "cz",
		"prepare": "husky install .dev/husky"
	},
	"keywords": [
		"javascript",
		"chrome-extension",
		"firefox-extension",
		"browser-extension",
		"extension",
		"chat-gpt",
		"gpt",
		"gpt4",
		"open-ai",
		"openai",
		"ai",
		"web",
		"npm"
	],
	"devDependencies": {
		"@commitlint/cli": "^18.4.2",
		"@parcel/config-webextension": "^2.10.3",
		"@parcel/packager-webextension": "^2.10.3",
		"@parcel/transformer-webextension": "^2.10.3",
		"@pigeon-posse/eslint-config": "^1.0.1",
		"@pigeon-posse/pkg-extended-scripts": "^0.3.0",
		"@playwright/test": "^1.40.0",
		"@release-it/bumper": "^4.0.2",
		"auto-changelog": "^2.4.0",
		"browserslist": "^4.22.1",
		"cli-markdown": "^3.0.5",
		"commitizen": "^4.3.0",
		"commitlint-config-gitmoji": "^2.3.1",
		"cz-customizable": "^7.0.0",
		"cz-emoji": "1.3.2-canary.2",
		"eslint": "^8.54.0",
		"eslint-plugin-align-assignments": "^1.1.2",
		"eslint-plugin-align-import": "^1.0.0",
		"eslint-plugin-html": "^7.1.0",
		"eslint-plugin-import": "^2.29.0",
		"eslint-plugin-jsdoc": "^39.9.1",
		"eslint-plugin-json": "^3.1.0",
		"eslint-plugin-promise": "^6.1.1",
		"husky": "^8.0.3",
		"inquirer": "^9.2.12",
		"lint-staged": "^15.1.0",
		"markdownlint": "^0.31.1",
		"npm-run-all": "^4.1.5",
		"parcel": "^2.10.3",
		"process": "^0.11.10",
		"release-it": "^17.0.0",
		"rimraf": "^4.4.1",
		"web-ext": "^7.8.0"
	},
	"browserslist": [
		">0.3%, defaults"
	],
	"@parcel/bundler-default": {
		"minBundles": 10000000
	},
	"commitlint": {
		"extends": [
			"gitmoji"
		],
		"rules": {
			"header-max-length": [
				0,
				"always",
				100
			]
		}
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-customizable"
		},
		"cz-customizable": {
			"config": ".dev/cz-config.cjs"
		}
	},
	"lint-staged": {
		"*.{js,cjs,mjs,jsx,ts,cts,mts,tsx,json}": "eslint"
	},
	"webExt": {
		"sourceDir": "dist",
		"run": {
			"startUrl": [
				"https://chat.openai.com/"
			]
		}
	},
	"extra": {
		"errorId": "pigeon-error",
		"path": {
			"releaseIt": ".release-it.json"
		},
		"licenseUrl": "https://github.com/pigeonposse/stylegpt/LICENSE",
		"chatWebMatch": "/chat\\.openai\\.com/",
		"chatPage": "https://chat.openai.com/",
		"store": {
			"chrome": {
