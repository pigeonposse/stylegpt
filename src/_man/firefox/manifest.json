{
	"name": "styleGPT",
	"version": "1.0.0",
	"description": "",
	"manifest_version": 2,
	"icons": {
		"96": "../../assets/img/icon-96.png"
	},
	"permissions": [
		"storage",
		"tabs"
	],
	"background": {
		"scripts": [
			"../../background/background.js"
		],
		"persistent": false
	},
	"browser_action": {
		"default_popup": "../../popup/popup.html"
	},
	"content_scripts": [
		{
			"matches": [
				"https://chat.openai.com",
				"https://chat.openai.com/*"
			],
			"js": [
				"../../content/content.js"
			],
			"css": [
				"../../content/content.css"
			],
			"run_at": "document_end"
		}
	]
}
